          "urlPath": "/api/v1/market/realtime",
          "fields": [
            { "jsonPath": "$.tickers[*].symbol", "name": "Symbol" },
            { "jsonPath": "$.tickers[*].last_price", "name": "Price" },
            { "jsonPath": "$.tickers[*].price_change", "name": "Change" },
            { "jsonPath": "$.tickers[*].price_change_pct", "name": "Change %" },
            { "jsonPath": "$.tickers[*].open", "name": "Open" },
            { "jsonPath": "$.tickers[*].high", "name": "High" },
            { "jsonPath": "$.tickers[*].low", "name": "Low" },
            { "jsonPath": "$.tickers[*].volume", "name": "Volume" },
            { "jsonPath": "$.tickers[*].quote_volume", "name": "Quote Vol" },
            { "jsonPath": "$.tickers[*].trades_count", "name": "Trades" },
            { "jsonPath": "$.tickers[*].updated_at", "name": "Updated" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": { "custom": { "align": "center", "displayMode": "auto", "filterable": true } },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Symbol" }, "properties": [{ "id": "custom.width", "value": 100 }, { "id": "custom.displayMode", "value": "color-text" }, { "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }] },
          { "matcher": { "id": "byName", "options": "Price" }, "properties": [{ "id": "unit", "value": "currencyUSD" }, { "id": "decimals", "value": 2 }] },
          { "matcher": { "id": "byName", "options": "Change" }, "properties": [{ "id": "unit", "value": "currencyUSD" }, { "id": "decimals", "value": 2 }, { "id": "custom.displayMode", "value": "color-text" }, { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 0 }] } }] },
          { "matcher": { "id": "byName", "options": "Change %" }, "properties": [{ "id": "unit", "value": "percent" }, { "id": "decimals", "value": 2 }, { "id": "custom.displayMode", "value": "color-background-solid" }, { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 0 }] } }] },
          { "matcher": { "id": "byName", "options": "Open" }, "properties": [{ "id": "unit", "value": "currencyUSD" }, { "id": "decimals", "value": 2 }] },
          { "matcher": { "id": "byName", "options": "High" }, "properties": [{ "id": "unit", "value": "currencyUSD" }, { "id": "decimals", "value": 2 }, { "id": "custom.displayMode", "value": "color-text" }, { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }] },
          { "matcher": { "id": "byName", "options": "Low" }, "properties": [{ "id": "unit", "value": "currencyUSD" }, { "id": "decimals", "value": 2 }, { "id": "custom.displayMode", "value": "color-text" }, { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }] },
          { "matcher": { "id": "byName", "options": "Volume" }, "properties": [{ "id": "unit", "value": "short" }, { "id": "decimals", "value": 2 }] },
          { "matcher": { "id": "byName", "options": "Quote Vol" }, "properties": [{ "id": "unit", "value": "currencyUSD" }, { "id": "decimals", "value": 0 }] },
          { "matcher": { "id": "byName", "options": "Trades" }, "properties": [{ "id": "unit", "value": "short" }] },
          { "matcher": { "id": "byName", "options": "Updated" }, "properties": [{ "id": "unit", "value": "dateTimeAsIso" }, { "id": "custom.displayMode", "value": "auto" }, { "id": "noValue", "value": "N/A" }] }
        ]
