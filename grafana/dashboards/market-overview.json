    {
      "id": 1,
      "title": "All Tickers - Real-time Data",
      "description": "Real-time ticker data from Kafka â†’ Redis pipeline. Shows all configured symbols.",
      "type": "table",
      "datasource": { "type": "marcusolsson-json-datasource", "uid": "fastapi" },
      "gridPos": { "h": 14, "w": 24, "x": 0, "y": 4 },
